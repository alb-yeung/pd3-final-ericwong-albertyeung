public class LinkedList{
     
    private class Node{
	//cost, houses, rent, owner
	private int cost;
	private int houses;
	private int rent;
	private int rampamt;//how much extra per house
	private String owner;
	
	public Node(int cost, int rent, int rampamt){
	    this.cost = cost;
	    this.houses = 0;
	    this.rent = rent;
	    this.rampamt = rampamt;	    
	}

	public setOwner(String name){
	    owner = name;
	}

    }


}